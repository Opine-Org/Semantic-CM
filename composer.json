{
    "name": "opine/semantic-cm",
    "type": "library",
    "description": "Semantic-CM, the Opine-PHP and Semantic-UI based Management System",
    "keywords": [
        "cms"
    ],
    "license": "MIT",
    "authors": [
        {
            "name"    : "Ryan Mahoney",
            "email"   : "ryan@virtuecenter.com",
            "homepage": "http://opine-php.org",
            "role"    : "Developer"
        }
    ],
    "require": {
        "php"                : ">=5.4.0",
        "symfony/yaml"       : "2.6.1",
        "cocur/slugify"      : "v0.2.3",
        "opine/http"         : "~2.0",
        "opine/db"           : "~2.0",
        "opine/cache"        : "~2.0",
        "opine/collection"   : "~2.0",
        "opine/config"       : "~3.0",
        "opine/container"    : "~2.0",
        "opine/form"         : "~2.0",
        "opine/pubsub"       : "~2.0",
        "opine/queue"        : "~2.0",
        "opine/search"       : "~2.0",
        "opine/layout"       : "~2.0",
        "opine/bundle"       : "~2.0",
        "opine/route"        : "~3.0",
        "opine/uploadwrapper": "~1.0",
        "opine/interfaces"   : "~1.0"
    },
    "autoload": {
        "classmap": [
            "src/Manager/app/controllers",
            "src/Manager/app/views",
            "src/Manager/app/models",
            "src/Manager/app/helpers"
        ]
    },
    "version": "1.5.11"
}